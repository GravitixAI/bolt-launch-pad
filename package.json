{
  "name": "bolt-launch-pad",
  "version": "0.1.0",
  "description": "A desktop application built with Electron, Vite, React, and TypeScript",
  "main": "dist/main/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "postinstall": "electron-builder install-app-deps && electron-rebuild -f -w better-sqlite3",
    "package": "pnpm build && electron-builder build --win --config",
    "package:dir": "pnpm build && electron-builder build --win --dir",
    "package:portable": "pnpm build && electron-builder build --win portable",
    "package:all": "pnpm build && electron-builder build --win nsis portable",
    "clean": "rimraf dist release",
    "rebuild": "electron-rebuild -f -w better-sqlite3"
  },
  "keywords": [
    "electron",
    "bolt",
    "launch-pad",
    "desktop",
    "windows"
  ],
  "author": "GravitixAI",
  "license": "MIT",
  "packageManager": "pnpm@10.20.0",
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "dependencies": {
    "@azure/msal-node": "^3.8.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "better-sqlite3": "^12.4.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-updater": "^6.6.2",
    "lucide-react": "^0.554.0",
    "mysql2": "^3.15.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-syntax-highlighter": "^16.1.0",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0"
  }
}
